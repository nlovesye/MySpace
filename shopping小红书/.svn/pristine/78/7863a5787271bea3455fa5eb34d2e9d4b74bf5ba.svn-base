    <script>  
          var selectIndex=0;
          $('.navList .item').click(function(){
              selectIndex=$(this).index();
              $(this).addClass('active').siblings().removeClass('active');
              $('.more-item .item').eq(selectIndex).addClass('active').siblings().removeClass('active');
               $('.main-content').eq(selectIndex).css('display','block').siblings().css('display','none');
          });
          $('.more-item .item').click(function(){
              selectIndex=$(this).index();
              $(this).addClass('active').siblings().removeClass('active');
              $('.navList .item').eq(selectIndex).addClass('active').siblings().removeClass('active');
              $('.main-content').eq(selectIndex).css('display','block').siblings().css('display','none');
              $('.more').css('transform','rotateZ(0deg)');
              $('.nav-list').css('display','block');
              $('.center .title,.more-item,.mask').css('display','none');
              bool=false;
          })
          var bool=false;
          $('.more').click(function(){
              if(!bool){
                $(this).css('transform','rotateZ(180deg)');
                $('.nav-list').css('display','none');
                $('.center .title,.more-item,.mask').css('display','block');
                bool=true;
              }else{
                $(this).css('transform','rotateZ(0deg)');
                $('.nav-list').css('display','block');
                $('.center .title,.more-item,.mask').css('display','none');
                bool=false;
              }
          })
    </script>
